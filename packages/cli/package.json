{
  "name": "@astar-network/swanky-cli",
  "version": "1.0.0-beta.1",
  "description": "Astar network WASM contract CLI tool",
  "author": "Astar network",
  "bin": {
    "swanky": "./bin/run"
  },
  "homepage": "https://github.com/AstarNetwork/swanky-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/AstarNetwork/swanky-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@astar-network/swanky-core": "^1.0.0-beta.1",
    "@astar-network/swanky-templates": "^1.0.0-beta.1",
    "@oclif/core": "1.20.2",
    "@oclif/plugin-help": "^5.1.17",
    "@oclif/plugin-plugins": "^2.1.4",
    "@oclif/plugin-version": "^1.1.3",
    "@polkadot/util-crypto": "^10.1.2",
    "chalk": "4",
    "change-case": "^4.1.2",
    "execa": "5.1.1",
    "fs-extra": "^10.1.0",
    "globby": "11",
    "inquirer": "^8.0.0",
    "listr2": "^5.0.1",
    "semver": "^7.3.7",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.8",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.0.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.11.9",
    "@types/semver": "^7.3.10",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "chai": "^4",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lerna": "^6.0.3",
    "mocha": "^10.0.0",
    "mocha-suppress-logs": "^0.3.1",
    "mochawesome": "^7.1.3",
    "oclif": "^3.2.24",
    "prettier": "^2.7.1",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "4.8.4"
  },
  "oclif": {
    "bin": "swanky",
    "dirname": "swanky",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v",
      "-V"
    ],
    "topics": {
      "init": {
        "description": "Init a new WASM contract project"
      }
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-version",
      "@oclif/plugin-plugins"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && rm -f tsconfig.tsbuildinfo && tsc -b",
    "lint": "eslint . --ext .ts --quiet --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --require mocha-suppress-logs --forbid-only -r ts-node/register \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": "https://github.com/AstarNetwork/swanky-cli/issues",
  "keywords": [
    "oclif",
    "swanky",
    "cli",
    "astar",
    "shiden",
    "wasm"
  ],
  "types": "dist/index.d.ts",
  "gitHead": "cccb996036cf2b6fbbe4e1f02c31079ba99fc517",
  "publishConfig": {
    "access": "public"
  }
}
